{
    "name": "studhalter-sveltekit-basetemplate",
    "version": "0.0.1",
    "description": "SvelteKit Template with UnoCSS, SASS, inlang",
    "keywords": ["sveltekit", "unocss", "iconify", "inlag", "paraglide-js"],
    "author": "Rainer Studhalter <rainer@studhalter.dev> (https://studhalter.dev)",
    "repository": {
        "type": "git",
        "url": "https://github.com/rainerstudhalter/studhalter-sveltekit-basetemplate.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rainerstudhalter/studhalter-sveltekit-basetemplate/issues"
    },
    "homepage": "https://github.com/rainerstudhalter/studhalter-sveltekit-basetemplate#readme",
    "type": "module",
    "scripts": {
        "dev": "vite dev --host",
        "debug": "dev --debug",
        "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
    },
    "devDependencies": {
        "@iconify/json": "^2.2.205",
        "@inlang/paraglide-js": "1.7.2",
        "@inlang/paraglide-sveltekit": "^0.6.12",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@unocss/extractor-svelte": "^0.59.4",
        "@unocss/transformer-directives": "^0.59.4",
        "@unocss/transformer-variant-group": "^0.59.4",
        "sass": "^1.76.0",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "unocss": "^0.59.4",
        "vite": "^5.0.3"
    },
    "private": true
}
